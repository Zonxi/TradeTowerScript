local ScreenGui = Instance.new("ScreenGui")
local main = Instance.new("Frame")
local label = Instance.new("TextLabel")
local tradetowerscript = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")
local afk = Instance.new("TextButton")

--Properties:

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

main.Name = "main"
main.Parent = ScreenGui
main.BackgroundColor3 = Color3.fromRGB(167, 201, 255)
main.Position = UDim2.new(0.0457875133, 0, 0.499404043, 0)
main.Size = UDim2.new(0, 638, 0, 314)
main.Active = true
main.Draggable = true

label.Name = "label"
label.Parent = main
