label.BackgroundTransparency = 100.000
label.Size = UDim2.new(0, 638, 0, 50)
label.Font = Enum.Font.SciFi
label.Text = "Zonxi Scripts | Trade Tower Script"
label.TextColor3 = Color3.fromRGB(0, 0, 0)
label.TextScaled = true
label.TextSize = 14.000
label.TextWrapped = true

tradetowerscript.Name = "tradetowerscript"
tradetowerscript.Parent = main
tradetowerscript.BackgroundColor3 = Color3.fromRGB(206, 237, 255)
tradetowerscript.Position = UDim2.new(0, 0, 0.273885339, 0)
tradetowerscript.Size = UDim2.new(0, 300, 0, 124)
tradetowerscript.Font = Enum.Font.SciFi
tradetowerscript.Text = "Zonxis Trade tower script"
tradetowerscript.TextColor3 = Color3.fromRGB(0, 0, 0)
tradetowerscript.TextScaled = true
tradetowerscript.TextSize = 14.000
tradetowerscript.TextWrapped = true
